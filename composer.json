{
  "name": "melihovv/eloquent-presence-verifier",
  "description": "An eloquent presence verifier implementation",
  "license": "MIT",
  "keywords": [
    "laravel",
    "eloquent",
    "presence",
    "verifier",
    "validation"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Alexander Melihov",
      "email": "amelihovv@ya.ru"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/database": "^6.0|7.0",
    "illuminate/support": "^6.0|7.0",
    "illuminate/validation": "^6.0|^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Melihovv\\EloquentPresenceVerifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Melihovv\\EloquentPresenceVerifier\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Melihovv\\EloquentPresenceVerifier\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
